{
  "version": "1.0.0-*",
  "description": "DNX consul client",
  "authors": [
    "Robert Tezli <robert@pixills.com>",
    "Andrei Cristof"
  ],
  "tags": [
    "consul",
    ".Net",
    "DNX"
  ],
  "projectUrl": "https://github.com/rtezli/consuldotnext",
  "licenseUrl": "https://github.com/rtezli/consuldotnext/blob/master/LICENSE",
  "tooling": {
    "defaultNamespace": "Pixills.Consul.Client"
  },
  "configurations": {
    "debug": {
      "compilationOptions": {
        "define": [
          "DEBUG",
          "TRACE"
        ],
        "optimize": false
      }
    },
    "release": {
      "compilationOptions": {
        "define": [
          "RELEASE"
        ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "dnxcore50": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-rc2-23513",
        "System.Collections": "4.0.11-rc1-update1",
        "System.Linq": "4.0.1-rc1-update1",
        "System.Runtime": "4.0.21-rc1-update1",
        "System.Threading": "4.0.11-rc1-update1"
      }
    }
  },
  "dependencies": {
    "Newtonsoft.Json": "8.0.2",
    "System.Net.Http": "4.0.1-rc2-23513",
    "System.Threading.Tasks": "4.0.11-rc1-update1"
  },
  "commands": {
    "build": "cd .. && grunt build"
  }
}
